<Window x:Class="WoWAHDataProject.GUI.DatabaseGUI.WindowImportToDatabase"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WoWAHDataProject.GUI.DatabaseGUI"
        mc:Ignorable="d"
        Title="WindowImportToDatabase" Height="215" Width="500">
    <Grid>
        <StackPanel Margin="25">
            <Button x:Name="BtnSelectImportCsvToDatabase" Click="BtnSelectImportCsvToDatabaseClick" Margin="10" Height="35" Width="250">Import CSVs to database</Button>
            <Button x:Name="BtnSelectImportMarketValuesToDatabase" Click="BtnSelectImportMarketValuesToDatabaseClick" Margin="10" Height="35" Width="250">Import market values to database</Button>
        </StackPanel>
        <Image x:Name="bottomLeftImage" HorizontalAlignment="Left" Width="200" Height="200" Margin="22,150,0,16"/>
    </Grid>
</Window>
